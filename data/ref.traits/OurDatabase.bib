@article{VEENENDAAL:td,
author = {VEENENDAAL, E M and SWAINE, M D and LECHA, R T and Walsh, M F and Abebrese, I K and OWUSU-AFRIYIE, K},
title = {{Responses of West African Forest Tree Seedlings to Irradiance and Soil Fertility}},
journal = {Functional Ecology},
year = {1996},
