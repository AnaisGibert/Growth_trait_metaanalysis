}

@article{Zhang:2011fe,
author = {Zhang, Junyan and Mycroft, Erin E and Adams, Greg and Reekie, Ed},
title = {{Why do genotypes of Picea glauca differ in their growth response to elevated CO₂?}},
journal = {Tree physiology},
year = {2011},
volume = {31},
number = {1},
pages = {16--21},
keywords = {Daniel Database},
doi = {10.1093/treephys/tpq097},
pmid = {21388998},
read = {Yes},
rating = {0},
date-added = {2014-04-10T23:47:28GMT},
date-modified = {2014-08-05T01:30:50GMT},
abstract = {Meta-analyses reveal that fast-growing species have a greater growth response to elevated CO(2) than slow-growing species. It is unknown whether this is a direct response or whether inter-specific differences in growth are simply correlated with other physiological or morphological differences among species that affect the growth response to CO(2). Here we use intra-specific variation in Picea glauca to examine the mechanistic basis for this relationship. Relative growth rate (RGR) of 29 genotypes grown at ambient (370 {\textmu}l l(-1)) or elevated (740 {\textmu}l 1(-1)) CO(2) was measured. Physiological and morphological traits describing differences in allocation, canopy structure, stomatal function and photosynthesis were determined. Most variation in RGR (74%) was explained by traits associated with canopy structure. Although there was a strong correlation between RGR(740) and RGR(370), we found no evidence that genotypes that grew fast at ambient CO(2) had a greater relative growth response to CO(2). Given that the pattern found at the intra-specific level differed from that reported at the inter-specific level, our results suggest that RGR per se does not affect the growth response to CO(2). Rather, the CO(2) growth response is determined by traits that may or may not be correlated with RGR.},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21388998},
local-url = {file://localhost/Users/anaisgibert/Dropbox/Library.papers3/Files/56/569332A8-163A-44AF-921A-D12EE77EAAE7.pdf},
file = {{569332A8-163A-44AF-921A-D12EE77EAAE7.pdf:/Users/anaisgibert/Dropbox/Library.papers3/Files/56/569332A8-163A-44AF-921A-D12EE77EAAE7.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1093/treephys/tpq097}}
}

