@article{CamaraZapata:2003df,
author = {C{\'a}mara Zapata, J M and Nieves, M and Cerd{\'a}, A},
title = {{Improvement in growth and salt resistance of lemon (Citrus limon) trees by an interstock-induced mechanism.}},
journal = {Tree physiology},
