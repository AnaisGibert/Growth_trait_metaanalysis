@article{palik_vertical_1993,
author = {Palik, B J and Pregitzer, K S},
title = {{The vertical development of early successional forests in northern Michigan, USA}},
journal = {Journal of Ecology},
year = {1993},
volume = {81},
number = {2},
pages = {271--285},
keywords = {Daniel Database},
read = {Yes},
