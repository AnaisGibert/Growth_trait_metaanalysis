number = {4},
pages = {735--742},
month = jul,
publisher = {Wiley},
keywords = {Daniel Database},
doi = {10.2307/2846146?ref=search-gateway:94ddc30fc74eff746e26b8c4b45ad653},
language = {English},
read = {Yes},
