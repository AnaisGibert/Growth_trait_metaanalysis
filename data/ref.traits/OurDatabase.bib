@article{Brouillette:2014hq,
author = {Brouillette, Larry C and Mason, Chase M and Shirk, Rebecca Y and Donovan, Lisa A},
title = {{Adaptive differentiation of traits related to resource use in a desert annual along a resource gradient.}},
journal = {The New phytologist},
year = {2014},
