@article{Buckley:2006ga,
author = {Buckley, T N and Roberts, D W},
title = {{How should leaf area, sapwood area and stomatal conductance vary with tree height to maximize growth?}},
journal = {Tree physiology},
year = {2006},
volume = {26},
number = {2},
pages = {145--157},
keywords = {Daniel Database},
doi = {10.1093/treephys/26.2.145},
read = {Yes},
